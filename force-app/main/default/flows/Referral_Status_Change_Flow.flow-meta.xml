                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Contacted</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Status_to_Contacted</targetReference>
            </connector>
            <label>Default Contact Value</label>
        </rules>
